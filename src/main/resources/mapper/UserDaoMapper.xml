<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.art.dao.UserDao">
    <select id="selectByUsername" resultType="com.art.entity.UserEntity">
        select * from vip_user where username=#{username} and #{password}
    </select>
</mapper>